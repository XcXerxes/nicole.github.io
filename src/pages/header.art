<div class="header">
    <div class="header-user col-md-10 offset-md-1 row">
      <div class="col-md-3 user-item">
        <div class="header-user__avatar">
          <img src="{{userInfo.avatar}}" />
          <div class="header-user__name text-center">
            <h3>{{userInfo.name}}</h3>
            <h4>{{userInfo.job}}</h4>
          </div>
        </div>
      </div>
      <div class="col-md-3 text-center user-item">
        <div class="header-user__contact text-center">
          <h4>个人信息</h4>
          <p>{{userInfo.experience}}</p>
          <p>{{userInfo.age}}</p>
          <p>{{userInfo.address}}</p>
          <p>{{userInfo.profession}}</p>
          </div>
      </div>
      <div class="col-md-3 text-center user-item">
        <div class="header-user__contact text-center">
          <h4>联系我</h4>
          <p>{{userInfo.education}}</p>
          <p>{{userInfo.phone}}</p>
          <p>{{userInfo.email}}</p>
          <p>{{userInfo.sex}}</p>
          </div>
      </div>
      <div class="col-md-3 user-item">
        <div class="header-user__follow">
          <h4 class="text-center">follow me</h4>
          <div class="row text-center">
            <div class="col-md-4 col-sm-4 col-4">
              <a href="http://wpa.qq.com/msgrd?v=3&uin=2860144252&site=在线客服&menu=yes">
                <i class="fa fa-qq"></i>
              </a>
            </div>
            <div class="col-md-4 col-sm-4 col-4">
              <a href="#">
                <i class="fa fa-wechat"></i>
              </a>
            </div>
            <div class="col-md-4 col-sm-4 col-4">
              <a href="#">
                <i class="fa fa-twitter"></i>
              </a>
            </div>
          </div>
          <div class="row text-center">
            <div class="col-md-4 col-sm-4 col-4">
              <a href="#">
                <i class="fa fa-weibo"></i>
              </a>
            </div>
            <div class="col-md-4 col-sm-4 col-4">
              <a href="#">
                <i class="fa fa-facebook-square"></i>
              </a>
            </div>
            <div class="col-md-4 col-sm-4 col-4">
              <a href="#">
                <i class="fa fa-linkedin"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>

