    <div class="tabs">
      <div class="tabs-container">
        <div class="tabs-slider__wrapper" id="tabSlider">
        </div>
        {{each userInfo.tabs}}
          <div class="tabs-div" type="{{$value.type}}">
            <a href="javascript:;">{{$value.title}}</a>
          </div>
        {{/each}}
      </div>
      <div>
        <div class="tabs-item__content active" data-type="all">
          <div class="tabs-item__body col-md-4 col-sm-12">
            <div class="tabs-item__media col-md-12">
